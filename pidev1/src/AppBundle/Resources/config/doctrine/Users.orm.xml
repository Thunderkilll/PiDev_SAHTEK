<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <indexes>
      <index name="id_centre" columns="id_centre"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="cin_3" columns="cin"/>
      <unique-constraint name="email" columns="email"/>
      <unique-constraint name="id_usr" columns="id_usr"/>
      <unique-constraint name="telephone" columns="telephone"/>
      <unique-constraint name="id_pharmacie" columns="id_pharmacie"/>
      <unique-constraint name="num_carte_serv" columns="num_carte_serv"/>
    </unique-constraints>
    <id name="idUsr" type="integer" column="id_usr">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="252" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="252" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="false"/>
    <field name="cin" type="integer" column="cin" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="252" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="integer" column="telephone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="user" type="string" column="user" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numCarteServ" type="integer" column="num_carte_serv" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idPharmacie" type="integer" column="id_pharmacie" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idCentre" type="integer" column="id_centre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
