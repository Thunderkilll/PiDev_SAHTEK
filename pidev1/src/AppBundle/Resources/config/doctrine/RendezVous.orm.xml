<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\RendezVous" table="rendez_vous">
    <indexes>
      <index name="id_medecin" columns="id_medecin"/>
    </indexes>
    <id name="cin" association-key="true"/>
    <field name="nom" type="string" column="nom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="false"/>
    <field name="telephone" type="integer" column="telephone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateReservation" type="datetime" column="Date_Reservation" nullable="true"/>
    <one-to-one field="cin" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="cin" referenced-column-name="cin"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idMedecin" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="id_medecin" referenced-column-name="id_usr"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
