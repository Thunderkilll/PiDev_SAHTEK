<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Articles" table="articles">
    <indexes>
      <index name="id_pat" columns="id_user"/>
    </indexes>
    <id name="idArticle" type="integer" column="id_article">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="integer" column="titre" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="subT" type="integer" column="sub_t" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descript" type="text" column="descript" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
